; status-code: 5

(defun main ()
  (set $status-code (if (< 1 2 3) (+ $status-code 1) 0))
  (set $status-code (if (< 1 2 1) 0 (+ $status-code 1)))
  (set $status-code (if (<= 1 1 2) (+ $status-code 1) 0))
  (set $status-code (if (> 2 1 0) (+ $status-code 1) 0))
  (set $status-code (if (>= 2 2 1 0) (+ $status-code 1) 0)))
